<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="easydel.gonew.mapper.addressDongs">
	<select id="selectAddressDongsByAddressGuName" parameterType="string"
		resultType="AddressDong">
		select		*
		from		ADDRESS_DONG
		where		dong_desc=#{dongDesc}
	</select>
</mapper>
